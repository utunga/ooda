{
  "name": "ooodaloops-server",
  "description": "OODA loops Server",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": "utunga <utunga@gmail.com>",
  "contributors": [
    "Will Murray <willdavidmurray@gmail.com>"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit",
    "upgrade-interactive": "npm-check --update",
    "deploy": "now --public --target production",
    "mongod": "mongod --config /usr/local/etc/mongod.conf"
  },
  "dependencies": {
    "@feathersjs/configuration": "^2.0.2",
    "@feathersjs/errors": "^3.3.2",
    "@feathersjs/express": "^1.2.5",
    "@feathersjs/feathers": "^3.2.1",
    "@feathersjs/socketio": "^3.2.4",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "feathers-mongoose": "^7.3.2",
    "feathers-nedb": "^3.1.0",
    "helmet": "^3.13.0",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.6.4",
    "nedb": "^1.8.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
